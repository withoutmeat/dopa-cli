{
  "name": "<%= appName %>",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "serve": "npx taco serve",
    "build": "npx taco build",
    <% if (test) { %>
    "test:unit": "jest",
    "test:gui": "majestic",
    <% } %>
    <% if (storybook) { %>
    "storybook": "start-storybook -p 3333",
    "build-storybook": "build-storybook",
    <% } %>
    <% if (eslint) { %>
    "lint": "eslint ./src --ext .ts,.tsx,.vue",
    "lint:fix": "tsc ./src --noEmit & eslint ./src --ext .ts,.tsx,.vue --fix",
    <% } %>
    "type-check": "tsc --noEmit",
    "type-check-watch": "yarn type-check --watch"
  },
  "keywords": [],
  "author": "withoutmeat <withoutmeat@qq.com>",
  "license": "ISC",
  "devDependencies": {
    "@taco-cli/babel-preset": "^0.0.0",
    "@taco-cli/core": "^0.0.0",
    <% if (storybook) { %>
    "@storybook/addon-actions": "6.2.3",
    "@storybook/addon-essentials": "6.2.3",
    "@storybook/addon-links": "6.2.3",
    "@storybook/builder-webpack5": "6.2.3",
    "@storybook/vue3": "6.2.3",
    <% } %>
    <% if (test) { %>
    "@types/jest": "^26.0.20",
    <% } %>
    <% if (eslint) { %>
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    <% } %>
    <% if (test) { %>
    "@vue/test-utils": "^2.0.0-rc.3",
    <% } %>
    <% if (eslint) { %>
    "eslint": "^7.21.0",
    "eslint-config-taco": "^0.0.0",
    <% } %>
    <% if (test) { %>
    "jest": "^26.6.3",
    "majestic": "^1.8.0",
    <% } %>
    "postcss": "^8.2.7",
    "postcss-custom-properties": "^11.0.0",
    "postcss-import": "^14.0.0",
    "postcss-loader": "^5.1.0",
    "postcss-nested": "^5.0.5",
    "prettier": "^2.2.1",
    <% if (tailwindcss) { %>
    "tailwindcss": "^2.0.3",
    <% } %>
    <% if (test) { %>
    "ts-jest": "^26.5.3",
    "vue-jest": "^5.0.0-alpha.7"
    <% } %>
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.13.10",
    "axios": "^0.21.1",
    "vue": "^3.0.7"
  }
}
